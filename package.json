{
  "name": "tweetdeck",
  "version": "1.0.0",
  "description": "Send tweets using tweetdeck, now right from your desktop!",
  "productName": "TweetDeck",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [
    "twitter",
    "tweetdeck",
    "tweet",
    "deck",
    "tweet deck"
  ],
  "author": "Cukmekerb",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-flatpak": "^6.0.0-beta.54",
    "electron": "^11.0.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
  
        {
          "name": "@electron-forge/maker-flatpak",
          "config": {
            "options": {
              "categories": [
                "Utility"
              ]
            }
          }
        }
      ]
    }
  }
}
